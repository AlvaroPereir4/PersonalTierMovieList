{
  "version": 2,
  "builds": [
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "/tierlistmovies/staticfiles"
      }
    },
    {
      "src": "tierlistmovies/tierlistmovies/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/styles/style.css",
      "dest": "/styles/style.css"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "tierlistmovies/tierlistmovies/wsgi.py"
    }
  ]
}
